{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    [
      "import",
      [
        {
          "libraryName": "antd",
          "libraryDirectory": "lib", // default: lib
          "style": true
        },
        {
          "libraryName": "antd-mobile",
          "libraryDirectory": "component",
        }
      ]
    ],
    "transform-decorators-legacy"
  ]
}